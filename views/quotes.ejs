<html>
    <head>
        <title>QuotingDojo</title>
    </head>
    <body>
        <h1>Here are the awesome quotes!</h1>
        <hr>
        <%for (i in quotes){%>
        <p>"<%=quotes[i].quote%>"</p>
        <p> - <%=quotes[i].name%> at <%=quotes[i]._id.getTimestamp()%>
        <hr>
        <%}%>




        <form action='/' method='get'>
        <input type='submit' id='submit' name='submit' value='Back to add quote!'>
        </form>
    </body>
</html>